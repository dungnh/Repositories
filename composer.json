{
    "name": "coliving/repositories",
    "description": "Flexable Laravel 6.x & 7.x & 8.x Repository Library.",
    "keywords": [
        "league",
        "repository"
    ],
    "homepage": "https://github.com/dungnh/Repositories",
    "license": "MIT",
    "authors": [
        {
            "name": "Dzung Nguyen",
            "email": "dungnh@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "illuminate/contracts": "^6.0 | ^7.0 | ^8.0",
        "illuminate/database": "^6.0 | ^7.0 | ^8.0"
    },
    "require-dev": {
        "scrutinizer/ocular": "~1.1",
        "phpunit/phpunit": "9.*",
        "orchestra/testbench": "5.0.*",
    },
    "autoload": {
        "psr-4": {
            "SebastianBerc\\Repositories\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SebastianBerc\\Repositories\\Test\\": "tests"
        },
        "classmap": [
            "tests/migrations"
        ]
    },
    "scripts": {
        "test": "phpunit"
    }
}
